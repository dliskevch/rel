{
  "commit-search-depth": 20,
  "include-v-in-tag": true,
  "include-component-in-tag": true,
  "tag-separator": "/",
  "bump-minor-pre-major": false,
  "bump-patch-for-minor-pre-major": false,
  "draft": false,
  "prerelease": false,
  "changelog-host": "https://github.com",
  "packages": {
    ".": {
      "release-type": "simple",
      "extra-files": [
        {
          "type": "yaml",
          "path": "helm-chart/Chart.yaml",
          "jsonpath": "$.appVersion"
        }
      ],
      "changelog-sections": [
        {
          "type": "feat",
          "section": "Features"
        },
        {
          "type": "fix",
          "section": "Fixes"
        },
        {
          "type": "chore",
          "hidden": true,
          "section": "Other"
        },
        {
          "type": "docs",
          "section": "Docs"
        },
        {
          "type": "refactor",
          "section": "Refactoring"
        },
        {
          "type": "build",
          "hidden": true,
          "section": "Build"
        },
        {
          "type": "ci",
          "hidden": true,
          "section": "CI"
        },
        {
          "type": "revert",
          "hidden": true,
          "section": "Reverts"
        },
        {
          "type": "style",
          "hidden": true,
          "section": "Styling"
        },
        {
          "type": "test",
          "hidden": true,
          "section": "Tests"
        }
      ]
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
